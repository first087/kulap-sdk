{
  "name": "kulap-sdk",
  "version": "0.1.0",
  "description": "Javascript SDK for Kulap DEX",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
