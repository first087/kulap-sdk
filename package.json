      "ts",
      "tsx",
      "js"
    ]
  },
  "author": "pisuth@elliptix.io",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "jest": "^26.6.3",
    "rollup": "^2.33.3",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "axios": "^0.21.0"
  }
}
